<?php
/**
 * Clase para cargar elementos estticos como:
 * 	imgenes
 * 	css
 *  javascript
 */

class Statics {
	public function imgSrc ( $file ) {
		return IMAGES_URL . $file;
	}
	
	public function jsSrc ( $file ) {
		return JS_URL . $file;
	}
	
	public function cssSrc ( $file ) {
		return CSS_URL . $file;
	}
}
?>